<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ingesup.android.projet.activites"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <uses-library android:name="com.google.android.maps"/>
        
        <activity
            android:name=".FindMyContactsActivity"
            android:label="@string/title_activity_find_my_contacts" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.ingesup.android.projet.activites.ProfilUtilisateurActivity" />
        <activity android:name="com.ingesup.android.projet.activites.ImpressionRapportBirtActivity" />
        <activity android:name="com.ingesup.android.projet.activites.RechercheContactsActivity" />       
    </application>

</manifest>